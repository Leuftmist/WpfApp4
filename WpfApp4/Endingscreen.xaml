<Window x:Class="WpfApp4.Endingscreen"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp4"
        mc:Ignorable="d"
        Title="Endingscreen" Height="450" Width="800">
    <Grid>
        <ListBox x:Name="Lb_left" HorizontalAlignment="Left" Height="250" Margin="123,0,0,0" VerticalAlignment="Center" Width="241" d:ItemsSource="{d:SampleData ItemCount=5}"
                 FontSize="32" FontWeight="Bold"/>
        <ListBox x:Name="Lb_right" HorizontalAlignment="Left" Height="250" Margin="400,0,0,0" VerticalAlignment="Center" Width="241" d:ItemsSource="{d:SampleData ItemCount=5}"
            FontSize="32" FontWeight="Bold"/>

    </Grid>
</Window>
